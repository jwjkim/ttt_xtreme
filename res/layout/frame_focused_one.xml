<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/game_focused_one"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center_horizontal|center_vertical"
    android:background="@color/white"
    android:visibility="gone" >

    <View
        android:id="@+id/frame_focus_one_a"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/sea_blue" />

    <View
        android:id="@+id/frame_focus_one_b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_toEndOf="@+id/frame_focus_one_a"
        android:layout_toRightOf="@+id/frame_focus_one_a" />

    <View
        android:id="@+id/frame_focus_one_c"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/frame_focus_one_b"
        android:layout_toRightOf="@+id/frame_focus_one_b" />

    <View
        android:id="@+id/frame_focus_one_d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/frame_focus_one_a"
        android:layout_marginTop="5dp" />

    <View
        android:id="@+id/frame_focus_one_e"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/frame_focus_one_b"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:layout_toEndOf="@+id/frame_focus_one_d"
        android:layout_toRightOf="@+id/frame_focus_one_d" />

    <View
        android:id="@+id/frame_focus_one_f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/frame_focus_one_c"
        android:layout_marginTop="5dp"
        android:layout_toEndOf="@+id/frame_focus_one_e"
        android:layout_toRightOf="@+id/frame_focus_one_e" />

    <View
        android:id="@+id/frame_focus_one_g"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/frame_focus_one_d" />

    <View
        android:id="@+id/frame_focus_one_h"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/frame_focus_one_e"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_toEndOf="@+id/frame_focus_one_g"
        android:layout_toRightOf="@+id/frame_focus_one_g" />

    <View
        android:id="@+id/frame_focus_one_i"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/frame_focus_one_f"
        android:layout_toEndOf="@+id/frame_focus_one_h"
        android:layout_toRightOf="@+id/frame_focus_one_h" />

    <View
        android:id="@+id/v_f_one_one"
        android:layout_width="5dp"
        android:layout_height="fill_parent"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:background="@color/black"
        android:contentDescription="@string/empty" />

    <View
        android:id="@+id/v_f_one_two"
        android:layout_width="5dp"
        android:layout_height="fill_parent"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:background="@color/black"
        android:contentDescription="@string/empty" />

    <View
        android:id="@+id/h_f_one_one"
        android:layout_width="fill_parent"
        android:layout_height="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/black"
        android:contentDescription="@string/empty" />

    <View
        android:id="@+id/h_f_one_two"
        android:layout_width="fill_parent"
        android:layout_height="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/black"
        android:contentDescription="@string/empty" />

</RelativeLayout>